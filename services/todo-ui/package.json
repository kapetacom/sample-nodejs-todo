{
    "name": "@kapeta/nodejs-sample-todo-ui",
    "version": "0.0.1",
    "main": "src/server/index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "start:dev": "NODE_ENV=development nodemon --ext ts,tsx,js,json,yaml,yml --watch '**/*' src/server/index.ts",
        "start:dev:docker": "NODE_ENV=development nodemon --exitcrash --ext ts,tsx,js,json,yaml,yml --watch '**/*' src/server/index.ts",
        "start": "NODE_ENV=production ts-node --transpile-only src/server/index.ts",
        "build": "webpack --config webpack.production.config.js",
        "format": "prettier --write .",
        "lint": "tsc --noEmit && eslint src/**/*.ts"
    },
    "prettier": "@kapeta/prettier-config",
    "nodemonConfig": {
        "watch": [
            "kapeta.yml",
            "webpack.*.js",
            "package.json",
            "tsconfig.json",
            "src/server",
            "src/server/.generated"
        ],
        "ext": "ts,tsx,js,jsx,json,yaml,yml",
        "exec": "ts-node --transpile-only"
    },
    "dependencies": {
        "@types/node": "^20",
        "@types/express": "^4",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "@tsconfig/node18": "^18.2.2",
        "@tsconfig/create-react-app": "^2.0.1",
        "express": "^4.17.1",
        "express-handlebars": "^5.3.3",
        "@kapeta/sdk-config": "^2",
        "@kapeta/sdk-proxy-route": "^2.1.1",
        "@kapeta/sdk-rest": "^1.0.1",
        "@kapeta/sdk-rest-client": "^3.2",
        "@kapeta/sdk-rest-route": "^2",
        "@kapeta/sdk-server": "^4"
    },
    "devDependencies": {
        "@babel/cli": "^7.23.0",
        "@babel/core": "^7.23.2",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.23.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-proposal-private-methods": "^7.18.6",
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@babel/plugin-transform-modules-commonjs": "^7.22.5",
        "@babel/preset-env": "^7.23.2",
        "@babel/preset-react": "^7.22.15",
        "@babel/preset-typescript": "^7.23.2",
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@kapeta/eslint-config": "^0.6.0",
        "@kapeta/prettier-config": "^0.6.0",
        "@mui/icons-material": "^5.14.8",
        "@mui/material": "^5.14.8",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
        "@types/express": "^4.17.17",
        "@types/node": "^20.2.5",
        "@types/react": "^18.0.27",
        "@types/react-dom": "^18.0.10",
        "assets-webpack-plugin": "^7.1.1",
        "babel-loader": "^9.1.3",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.8.1",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^8.52.0",
        "eslint-config-prettier": "^8.8.0",
        "express": "^4.18.2",
        "html-webpack-plugin": "^5.5.0",
        "less": "^4.2.0",
        "less-loader": "^11.1.3",
        "mini-css-extract-plugin": "^2.7.6",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-refresh": "^0.14.0",
        "react-use": "^17.4.0",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "terser-webpack-plugin": "^5.3.6",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-middleware": "^6.1.1",
        "webpack-hot-middleware": "^2.25.4",
        "sass": "^1.69.4",
        "@kapeta/sdk-web-rest-client": "^2.1",
        "copy-webpack-plugin": "^11.0.0",
        "msw": "^2.0.6"
    },
    "msw": {
        "workerDirectory": "src/mocks"
    },
    "type": "commonjs"
}